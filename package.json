{
  "name": "openchat",
  "version": "1.0.2",
  "description": "Open Source Communication Platform",
  "main": "server.js",
  "repository": "https://github.com/reesvarney/OpenChat",
  "scripts": {
    "start": "node server",
    "showmcu": "node server showmcu",
    "createSSL": "openssl req -nodes -new -x509 -keyout ./ssl/server.key -out ./ssl/server.cert",
    "addAdmin": "node ./scripts/create_user",
    "liveDev": "nodemon server.js -i \"**/views/**\" -i \"**.json\" --delay 5",
    "setSecret": "node ./scripts/set_secret",
    "setup": "npm i && npm run createSSL && npm run addAdmin && npm run setSecret && exit 0"
  },
  "author": "RV",
  "license": "GPL-3.0",
  "dependencies": {
    "anchorme": "^2.1.2",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "ffmpeg-static": "^4.2.6",
    "ffmpeg-stream": "^0.6.0",
    "helmet": "^3.23.3",
    "jsdom": "^16.3.0",
    "moment": "^2.27.0",
    "open-graph-scraper": "^4.1.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "peer": "^0.5.3",
    "puppeteer": "^3.1.0",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.2.0"
  }
}
